#!/bin/bash

# Check if the first argument is %+Y
if [[ "$1" == "+%Y" ]]; then
    echo "2025"
    exit 0
fi

# Check if the first argument is +%s
if [[ "$1" == "+%s" ]]; then
    echo "1754506801"
    exit 0
fi

if [[ "$1" == "--date" && "$2" == "Aug 06 12:00:00 2025" ]]; then
    echo "1754506800"
    exit 0
fi

echo "foo"
