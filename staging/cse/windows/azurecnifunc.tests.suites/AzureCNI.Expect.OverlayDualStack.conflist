{
    "cniVersion": "0.3.0",
    "name": "azure",
    "adapterName": "",
    "plugins": [
        {
            "type": "azure-vnet",
            "mode": "bridge",
            "bridge": "azure0",
            "capabilities": {
                "portMappings": true,
                "dns": true
            },
            "ipam": {
                "type": "azure-cns",
                "mode": "dualStackOverlay"
            },
            "dns": {
                "Nameservers": [
                    "10.0.0.10",
                    "168.63.129.16"
                ],
                "Search": [
                    "svc.cluster.local"
                ]
            },
            "AdditionalArgs": [
                {
                    "Name": "EndpointPolicy",
                    "Value": {
                        "Type": "OutBoundNAT",
                        "ExceptionList": [
                            "10.244.0.0/16"
                        ]
                    }
                },
                {
                    "Name": "EndpointPolicy",
                    "Value": {
                        "Type": "ACL",
                        "Action": "Allow",
                        "Direction": "In",
                        "Priority": 65500
                    }
                },
                {
                    "Name": "EndpointPolicy",
                    "Value": {
                        "Type": "ACL",
                        "Action": "Allow",
                        "Direction": "Out",
                        "Priority": 65500
                    }
                },
                {
                    "Name": "EndpointPolicy",
                    "Value": {
                        "Type": "OutBoundNAT",
                        "ExceptionList": [
                            "fd12:3456::/64"
                        ]
                    }
                },
                {
                    "Name": "EndpointPolicy",
                    "Value": {
                        "Type": "ACL",
                        "Protocols": "6",
                        "Action": "Block",
                        "Direction": "Out",
                        "RemoteAddresses": "168.63.129.16/32",
                        "RemotePorts": "80",
                        "Priority": 200,
                        "RuleType": "Switch"
                    }
                },
                {
                    "Name": "EndpointPolicy",
                    "Value": {
                        "Type": "ACL",
                        "Action": "Allow",
                        "Direction": "In",
                        "Priority": 65500
                    }
                },
                {
                    "Name": "EndpointPolicy",
                    "Value": {
                        "Type": "ACL",
                        "Action": "Allow",
                        "Direction": "Out",
                        "Priority": 65500
                    }
                }
            ],
            "windowsSettings": {
                "enableLoopbackDSR": true
            }
        }
    ]
}