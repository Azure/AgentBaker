{
  "cniVersion": "0.3.0",
  "name": "azure",
  "adapterName": "",
  "plugins": [
    {
      "type": "azure-vnet",
      "mode": "bridge",
      "bridge": "azure0",
      "executionMode": "v4swift",
      "capabilities": {
        "portMappings": true,
        "dns": true
      },
      "ipam": {
        "type": "azure-cns",
        "mode": "dualStackOverlay"
      },
      "dns": {
        "Nameservers": [
          "10.0.0.10",
          "168.63.129.16"
        ],
        "Search": [
          "svc.cluster.local"
        ]
      },
      "AdditionalArgs": [
        {
          "Name": "EndpointPolicy",
          "Value": {
            "Type": "OutBoundNAT",
            "ExceptionList": []
          }
        },
        {
          "Name": "EndpointPolicy",
          "Value": {
            "Type": "ROUTE",
            "DestinationPrefix": "10.0.0.0/8",
            "NeedEncap": true
          }
        }
      ]
    }
  ]
}