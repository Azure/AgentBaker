{
  "plugin": "custom",
  "pluginConfig": {
    "invoke_interval": "30m",
    "timeout": "120s",
    "max_output_length": 1000,
    "concurrency": 1,
    "enable_message_change_based_condition_update": false
  },
  "source": "node-egress-monitor",
  "metricsReporting": true,
  "conditions": [],
  "rules": [
    {
      "type": "temporary",
      "reason": "EgressBlocked",
      "path": "/etc/node-problem-detector.d/plugin/check_egress_wrapper.sh",
      "timeout": "120s"
    }
  ]
}