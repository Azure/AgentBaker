{
  "plugin": "custom",
  "pluginConfig": {
    "invoke_interval": "2s",
    "timeout": "25s",
    "max_output_length": 512,
    "concurrency": 1,
    "enable_message_change_based_condition_update": true
  },
  "source": "custom-scheduledevents-consolidated-preempt-plugin-monitor",
  "metricsReporting": true,
  "conditions": [],
  "rules": [
    {
      "type": "temporary",
      "condition": "PreemptScheduled",
      "reason": "PreemptScheduled",
      "path": "/etc/node-problem-detector.d/plugin/check_preempt.sh",
      "timeout": "25s"
    }
  ]
}
